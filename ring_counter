#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x556f56ae8f00 .scope module, "tb_ring" "tb_ring" 2 3;
 .timescale -9 -12;
v0x556f56af9970_0 .var "Clock", 0 0;
v0x556f56af9a40_0 .net "Count_out", 3 0, L_0x556f56af9c10;  1 drivers
v0x556f56af9b10_0 .var "Reset", 0 0;
S_0x556f56ae9080 .scope module, "uut" "ring_counter" 2 13, 3 2 0, S_0x556f56ae8f00;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "Clock"
    .port_info 1 /INPUT 1 "Reset"
    .port_info 2 /OUTPUT 4 "Count_out"
L_0x556f56af9c10 .functor BUFZ 4, v0x556f56af9770_0, C4<0000>, C4<0000>, C4<0000>;
v0x556f56ab2030_0 .net "Clock", 0 0, v0x556f56af9970_0;  1 drivers
v0x556f56af9690_0 .net "Count_out", 3 0, L_0x556f56af9c10;  alias, 1 drivers
v0x556f56af9770_0 .var "Count_temp", 3 0;
v0x556f56af9830_0 .net "Reset", 0 0, v0x556f56af9b10_0;  1 drivers
E_0x556f56ae9600/0 .event edge, v0x556f56af9830_0;
E_0x556f56ae9600/1 .event posedge, v0x556f56ab2030_0;
E_0x556f56ae9600 .event/or E_0x556f56ae9600/0, E_0x556f56ae9600/1;
    .scope S_0x556f56ae9080;
T_0 ;
    %wait E_0x556f56ae9600;
    %load/vec4 v0x556f56af9830_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x556f56af9770_0, 0, 4;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x556f56ab2030_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x556f56af9770_0;
    %parti/s 3, 0, 2;
    %load/vec4 v0x556f56af9770_0;
    %parti/s 1, 3, 3;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x556f56af9770_0, 0, 4;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x556f56ae8f00;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556f56af9970_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x556f56ae8f00;
T_2 ;
    %delay 10000, 0;
    %load/vec4 v0x556f56af9970_0;
    %inv;
    %store/vec4 v0x556f56af9970_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x556f56ae8f00;
T_3 ;
    %vpi_call 2 26 "$dumpfile", "ring_counter.vcd" {0 0 0};
    %vpi_call 2 27 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x556f56ae8f00 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556f56af9b10_0, 0, 1;
    %delay 50000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556f56af9b10_0, 0, 1;
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench.v";
    "ring_counter.v";
